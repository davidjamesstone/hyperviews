#!/bin/bash

cat "examples/demo-view.html" | bin/cmd.js --mode browser > examples/demo-view.js
cat "examples/example.html" | bin/cmd.js --mode browser > examples/example.js
cat "examples/test.html" | bin/cmd.js --mode browser > examples/test.js
cat "examples/todo-view.html" | bin/cmd.js --mode browser > examples/todo-view.js
